{
  "apps" : [{
    "name"        : "ohif-viewer",
    "script"      : "main.js",
    "watch"       : true,
    "merge_logs"  : true,
    "cwd"         : "/app/bundle/",
    "env": {
      "METEOR_SETTINGS": {
		"servers": {
		  "dicomWeb": [
		    {
		      "name": "Dcm4chee",
		      "wadoUriRoot": "http://dcm4chee_arc_1:8080/dcm4chee-arc/aets/DCM4CHEE/wado",
		      "qidoRoot": "http://dcm4chee_arc_1:8080/dcm4chee-arc/aets/DCM4CHEE/rs",
		      "wadoRoot": "http://dcm4chee_arc_1:8080/dcm4chee-arc/aets/DCM4CHEE/rs",
		      "qidoSupportsIncludeField": false,
		      "imageRendering": "wadouri",
		      "thumbnailRendering": "wadors",
		      "requestOptions": {
			"auth": "admin:admin",
			"logRequests": false,
			"logResponses": false,
			"logTiming": false
		      }
		    }
		  ]
		},
		"defaultServiceType": "dicomWeb",
		  "public": {
		    "verifyEmail": false,
		    "ui": {
			"leftSidebarOpen": false,
			"displaySetNavigationLoopOverSeries": false,
			"displaySetNavigationMultipleViewports": true,
			"autoPositionMeasurementsTextCallOuts": "TRLB",
			"studyListFunctionsEnabled": true,
			"studyListDateFilterNumDays": 20000,
			"showStackLoadingProgressBar": true
		    }
		  },

		"proxy": {
		  "enabled": true
		}
      }
    }
  }]
}
